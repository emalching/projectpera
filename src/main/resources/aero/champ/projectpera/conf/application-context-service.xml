<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="timesheetReportGenerator" class="aero.champ.projectpera.scheduler.scheduled.BiMonthlyGenerator">
	</bean>
	
	<bean id="dailyTimeRetriever" class="aero.champ.projectpera.scheduler.scheduled.DailyTimeRetriever">
	</bean>
	
	<bean id="employeeCsvParser" class="aero.champ.projectpera.parser.EmployeeCSVParser">
		<property name="employeeRepository"	ref="employeeRepository" />
	</bean>
	
	<bean id="employeeTimeRecorder" class="aero.champ.projectpera.parser.EmployeeTimeRecorder">
		 <property name="timesheetReportGenerator">
            <ref bean="timesheetReportGenerator" />
        </property>
	</bean>

</beans>